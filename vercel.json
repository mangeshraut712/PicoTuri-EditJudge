{
  "version": 2,
  "name": "picoturi-editjudge",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12",
    "NODE_VERSION": "18.x",
    "PYTHONUNBUFFERED": "1",
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "api/**/*.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
